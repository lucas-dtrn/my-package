{
	"compilerOptions": {
		/* Base Options: */
		"esModuleInterop": true,
		"skipLibCheck": true,
		"target": "es2022",
		"allowJs": true,
		"moduleDetection": "force",
		/* Strictness */
		"strict": true,
		"noUncheckedIndexedAccess": true,
		"module": "ESNext",
		"noEmit": true,
		/* If your code runs in the DOM: */
		"lib": ["es2022", "dom", "dom.iterable"]
	}
}
